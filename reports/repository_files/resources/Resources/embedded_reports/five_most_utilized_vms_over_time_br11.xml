            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="inputControl">
            <folder>/Resources/embedded_reports/five_most_utilized_vms_over_time_br11_files</folder>
            <name>P_VM_Type</name>
            <version>0</version>
            <label>VM Type</label>
            <creationDate>2011-12-27T15:09:34.523+02:00</creationDate>
            <updateDate>2011-12-27T15:09:34.523+02:00</updateDate>
            <type>4</type>
            <mandatory>true</mandatory>
            <readOnly>false</readOnly>
            <visible>true</visible>
            <query>
                <localResource xsi:type="query">
                    <folder>/Resources/embedded_reports/five_most_utilized_vms_over_time_br11_files/P_VM_Type_files</folder>
                    <name>type</name>
                    <version>0</version>
                    <label>type</label>
                    <creationDate>2011-12-27T15:09:34.523+02:00</creationDate>
                    <updateDate>2011-12-27T15:09:34.523+02:00</updateDate>
                    <language>sql</language>
                    <queryString>SELECT v3_0_enum_translator_view.value, v3_0_enum_translator_view.enum_key,  0 as sort 
FROM v3_0_enum_translator_view, v3_0_vm_configuration_view
WHERE v3_0_vm_configuration_view.vm_type = v3_0_enum_translator_view.enum_key
AND v3_0_enum_translator_view.enum_type = 'VM_TYPE' 
AND v3_0_vm_configuration_view.cluster_id in (SELECT v3_0_cluster_configuration_view.cluster_id 
			            FROM v3_0_cluster_configuration_view 
			            WHERE v3_0_cluster_configuration_view.datacenter_id = cast($P{P_DataCenter_ID} as uuid))
AND v3_0_vm_configuration_view.cluster_id = CASE  $P{P_Cluster_ID}
				WHEN  '11111111-1111-1111-1111-111111111111' THEN v3_0_vm_configuration_view.cluster_id 
				ELSE cast($P{P_Cluster_ID} as uuid)
			             END
$P!{active_hosts_select}
UNION ALL
SELECT 'All', -1, 1
FROM v3_0_enum_translator_view
ORDER BY sort DESC</queryString>
                    <dataSource>
                        <uri>/Resources/JDBC/data_sources/ovirt</uri>
                    </dataSource>
                </localResource>
            </query>
            <queryVisibleColumn>value</queryVisibleColumn>
            <queryValueColumn>enum_key</queryValueColumn>
        </localResource>
