<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="cluster_capactity_vs_usage_cores_dr29" language="groovy" pageWidth="418" pageHeight="167" whenNoDataType="AllSectionsNoDetail" columnWidth="418" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
    <property name="ireport.zoom" value="1.0"/>
    <property name="ireport.x" value="0"/>
    <property name="ireport.y" value="0"/>
    <property name="ireport.jasperserver.reportUnit" value="/Resources/embedded_reports/webadmin_dashboards/cluster_dashboard/cluster_capactity_vs_usage_cores_dr29"/>
    <property name="ireport.jasperserver.url" value="http://localhost:8080/jasperserver/services/repository"/>
    <property name="net.sf.jasperreports.chart.theme" value="ReportsLineBarChartTheme"/>
    <parameter name="P_Cluster_ID" class="java.lang.String">
        <defaultValueExpression><![CDATA[]]></defaultValueExpression>
    </parameter>
    <queryString>
        <![CDATA[SELECT avg_host.history_datetime, host_cpu_cores, vm_cpu_cores
FROM
(SELECT history_datetime,
       SUM(coalesce(v3_0_host_configuration_view.number_of_cores,0)) AS host_cpu_cores
FROM v3_0_host_samples_history_view
         INNER JOIN v3_0_host_configuration_view
             ON (v3_0_host_configuration_view.history_id = v3_0_host_samples_history_view.host_configuration_version)
         INNER JOIN v3_0_latest_host_configuration_view a
             ON (a.host_id = v3_0_host_samples_history_view.host_id)
WHERE v3_0_host_samples_history_view.host_status = 1
      AND a.cluster_id = cast($P{P_Cluster_ID} as UUID)
GROUP BY history_datetime) as avg_host INNER JOIN
(SELECT history_datetime,
        SUM(coalesce(v3_0_vm_configuration_view.cpu_per_socket,0)*coalesce(v3_0_vm_configuration_view.number_of_sockets,0)) AS vm_cpu_cores
 FROM v3_0_vm_samples_history_view
          INNER JOIN v3_0_vm_configuration_view
              ON (v3_0_vm_configuration_view.history_id = v3_0_vm_samples_history_view.vm_configuration_version)
          INNER JOIN v3_0_latest_vm_configuration_view a
              ON (a.vm_id = v3_0_vm_samples_history_view.vm_id)
 WHERE v3_0_vm_samples_history_view.vm_status = 1
       AND a.cluster_id = cast($P{P_Cluster_ID} as UUID)
 GROUP BY history_datetime) as avg_vms ON (avg_vms.history_datetime = avg_host.history_datetime)]]>
    </queryString>
    <field name="history_datetime" class="java.sql.Timestamp"/>
    <field name="host_cpu_cores" class="java.lang.Long"/>
    <field name="vm_cpu_cores" class="java.lang.Long"/>
    <summary>
