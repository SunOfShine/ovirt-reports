    <queryString>
        <![CDATA[SELECT
      v3_2_configuration_history_vms.cluster_id
       ,v3_2_configuration_history_vms.vm_id
       ,v3_2_configuration_history_vms.vm_name
       ,CASE
        WHEN v3_2_configuration_history_vms.delete_date IS NULL THEN 0
        ELSE 1
    END as is_deleted
       ,SUM(CASE WHEN v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_status in (3,0) THEN coalesce(v3_2_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END)AS unplanned_downtime_mins
       ,SUM(CASE WHEN v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1 THEN coalesce(v3_2_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END)AS uptime_mins
       ,Sum(v3_2_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status) as total

FROM v3_2_statistics_vms_resources_usage_$P!{table_name}
INNER JOIN v3_2_configuration_history_vms
     ON (v3_2_configuration_history_vms.vm_id = v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_id)
WHERE
 v3_2_configuration_history_vms.cluster_id in (SELECT cluster_id
                         FROM v3_2_configuration_history_clusters
                         Where datacenter_id = cast($P{P_DataCenter_ID} as uuid))
AND v3_2_configuration_history_vms.cluster_id = CASE $P{P_Cluster_ID}
                           WHEN NULL THEN v3_2_configuration_history_vms.cluster_id
                       WHEN '11111111-1111-1111-1111-111111111111' THEN v3_2_configuration_history_vms.cluster_id
                       ELSE cast($P{P_Cluster_ID} as uuid)
                       END
AND v3_2_configuration_history_vms.vm_type = 1
AND high_availability = TRUE
AND history_datetime >= cast($P{P_Start_Date} as timestamp)
AND history_datetime <= CASE
                           WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                           WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                           WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                           WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                       END
AND v3_2_configuration_history_vms.history_id in (SELECT max(a.history_id)
                          FROM v3_2_configuration_history_vms a
                          GROUP BY a.vm_id)
$P!{is_deleted}
GROUP BY  v3_2_configuration_history_vms.cluster_id
      ,v3_2_configuration_history_vms.vm_id
      ,v3_2_configuration_history_vms.vm_name
      ,CASE
        WHEN v3_2_configuration_history_vms.delete_date IS NULL THEN 0
        ELSE 1
      END
HAVING SUM(CASE WHEN v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1 THEN coalesce(v3_2_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END) > 0
ORDER BY is_deleted ASC, cast(SUM(CASE WHEN v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_status in (3,0) THEN coalesce(v3_2_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END) as float)/cast(SUM(coalesce(v3_2_statistics_vms_resources_usage_$P!{table_name}.minutes_in_status,0)) as float) DESC]]>
    </queryString>
