    <queryString>
        <![CDATA[SELECT v3_2_configuration_history_hosts.cluster_id
      ,v3_2_configuration_history_hosts.host_id
      ,v3_2_configuration_history_hosts.host_name
      ,CASE
    WHEN v3_2_configuration_history_hosts.delete_date IS NULL THEN 0
    ELSE 1
       END as is_deleted
      ,SUM(CASE WHEN v3_2_statistics_hosts_resources_usage_$P!{table_name}.host_status = 2 THEN coalesce(v3_2_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END) AS planned_downtime_mins
      ,SUM(CASE WHEN v3_2_statistics_hosts_resources_usage_$P!{table_name}.host_status = 3 THEN coalesce(v3_2_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END) AS unplanned_downtime_mins
      ,SUM(CASE WHEN v3_2_statistics_hosts_resources_usage_$P!{table_name}.host_status = 1 THEN coalesce(v3_2_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END) AS uptime_mins
      ,SUM(coalesce(v3_2_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,0)) as total
FROM v3_2_statistics_hosts_resources_usage_$P!{table_name}
INNER JOIN v3_2_configuration_history_hosts
     ON (v3_2_configuration_history_hosts.host_id = v3_2_statistics_hosts_resources_usage_$P!{table_name}.host_id)
WHERE
v3_2_configuration_history_hosts.cluster_id in (SELECT cluster_id
                         FROM v3_2_configuration_history_clusters
                         Where datacenter_id = cast($P{P_DataCenter_ID} as uuid))
AND v3_2_configuration_history_hosts.cluster_id = CASE $P{P_Cluster_ID}
                           WHEN NULL THEN v3_2_configuration_history_hosts.cluster_id
                       WHEN '11111111-1111-1111-1111-111111111111' THEN v3_2_configuration_history_hosts.cluster_id
                       ELSE cast($P{P_Cluster_ID} as uuid)
                       END
AND v3_2_configuration_history_hosts.host_type = CASE $P{P_Host_Type}
                    WHEN -1 THEN v3_2_configuration_history_hosts.host_type
                    ELSE $P{P_Host_Type}
                    END
AND v3_2_configuration_history_hosts.history_id in (SELECT max(a.history_id)
                        FROM v3_2_configuration_history_hosts a
                        GROUP BY a.host_id)
AND history_datetime >= cast($P{P_Start_Date} as timestamp)
AND history_datetime <= CASE
                           WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                           WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                           WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                           WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                       END
$P!{is_deleted}
GROUP BY
      v3_2_configuration_history_hosts.cluster_id
      ,v3_2_configuration_history_hosts.host_id
      ,v3_2_configuration_history_hosts.host_name
      ,v3_2_configuration_history_hosts.delete_date
ORDER BY is_deleted ASC,
     cast(SUM(CASE WHEN v3_2_statistics_hosts_resources_usage_$P!{table_name}.host_status = 3 THEN coalesce(v3_2_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,0) ELSE 0 END) as float)/cast(SUM(coalesce(v3_2_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,0)) as float) DESC]]>
    </queryString>
