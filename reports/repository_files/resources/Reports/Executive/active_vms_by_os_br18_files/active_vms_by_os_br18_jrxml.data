                                                                WHEN CAST('11111111-1111-1111-1111-111111111111' as uuid) THEN v3_3_configuration_history_vms.cluster_id
                                                                ELSE CAST($P{P_Cluster_ID} as uuid)
                                                            END
            AND v3_3_configuration_history_vms.vm_type = CASE
