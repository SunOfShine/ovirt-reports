    <queryString>
            <![CDATA[-- BR18 - Datacenter aggregation level
-- VMs Count by OS (Windows vs Windows)
select distinct $P!{calendar_column} as the_date, coalesce(os_type,'RHEL','') as os_type, vm_count
from          (SELECT distinct CASE
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'UNKN%' THEN 'Unknown OS'
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'WIND%' THEN 'Windows'
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'RHEL%' THEN 'RHEL'
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'OTHER L%' THEN 'Other Linux'
                           ELSE 'Other OS'
                    END AS os_type,
                   history_datetime AS time,
                   COUNT(DISTINCT v3_2_configuration_history_vms.vm_id) AS vm_count
        FROM    v3_2_statistics_vms_resources_usage_$P!{table_name}
                  INNER JOIN v3_2_configuration_history_vms
                   ON (v3_2_configuration_history_vms.history_id = v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_configuration_version)
                       INNER JOIN v3_2_enum_translator enum_os_type
                       ON (enum_os_type.enum_key = v3_2_configuration_history_vms.operating_system AND enum_os_type.enum_type = 'OS_TYPE')
                  INNER JOIN v3_2_configuration_history_vms a
                   ON (a.vm_id = v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_id)
        WHERE    v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_status = 1
                 AND v3_2_configuration_history_vms.cluster_id in (SELECT cluster_id
                                     FROM v3_2_configuration_history_clusters
                                     Where datacenter_id = CAST($P{P_DataCenter_ID} as uuid))
                 AND v3_2_configuration_history_vms.cluster_id = CASE CAST($P{P_Cluster_ID} as uuid)
                                   WHEN NULL THEN v3_2_configuration_history_vms.cluster_id
                                   WHEN CAST('11111111-1111-1111-1111-111111111111' as uuid) THEN v3_2_configuration_history_vms.cluster_id
                                   ELSE CAST($P{P_Cluster_ID} as uuid)
                                   END
                AND v3_2_configuration_history_vms.vm_type = CASE
                                   WHEN $P{P_VM_Type} = -1 THEN v3_2_configuration_history_vms.vm_type
                                   ELSE $P{P_VM_Type}
                                END
                AND a.history_id in (SELECT max(b.history_id)
                             FROM v3_2_configuration_history_vms b
                             GROUP BY b.vm_id)
                AND history_datetime >= cast($P{P_Start_Date} as date)
                AND history_datetime <= CASE                                                                                                                                                             WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                           WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                           WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                           WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                       END
                AND CASE
                    WHEN $P{is_deleted} like 'AND%' THEN a.delete_date IS NULL
                    ELSE a.delete_date IS NULL or a.delete_date IS NOT NULL
                    END
        GROUP BY CASE
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'UNKN%' THEN 'Unknown OS'
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'WIND%' THEN 'Windows'
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'RHEL%' THEN 'RHEL'
                           WHEN UPPER(coalesce(enum_os_type.value,'Unknown OS')) like 'OTHER L%' THEN 'Other Linux'
                           ELSE 'Other OS'
                    END, history_datetime) as all_query RIGHT OUTER JOIN calendar
                ON (all_query.time = calendar.$P!{calendar_column})
where $P!{calendar_column} >= cast($P{P_Start_Date} as date)
      AND $P!{calendar_column} <= CASE                                                                                                                                                             WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                         WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                         WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                         WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                     END
order by $P!{calendar_column}, os_type]]>
        </queryString>
        <field name="the_date" class="java.sql.Timestamp"/>
        <field name="os_type" class="java.lang.String"/>
        <field name="vm_count" class="java.lang.Long"/>
    </subDataset>
