    <queryString>
        <![CDATA[-- Datacenter aggregation level
--------
-- BR22 - Hosts Count by OS Type versus OS Version

select distinct $P!{calendar_column} as the_date, coalesce(os_type,max(os_type) over (partition by 1),'') as os_type, host_count
FROM   (SELECT   history_datetime,
                 coalesce(v3_3_configuration_history_hosts.host_os, 'Unknown') as os_type,
                 COUNT(DISTINCT v3_3_configuration_history_hosts.host_id) AS host_count

        FROM    v3_3_statistics_hosts_resources_usage_$P!{table_name}
                INNER JOIN v3_3_configuration_history_hosts
                  ON (v3_3_configuration_history_hosts.history_id = v3_3_statistics_hosts_resources_usage_$P!{table_name}.host_configuration_version)
                INNER JOIN v3_3_configuration_history_hosts a
                  ON (a.host_id = v3_3_statistics_hosts_resources_usage_$P!{table_name}.host_id)

        WHERE
        v3_3_configuration_history_hosts.cluster_id in (SELECT v3_3_configuration_history_clusters.cluster_id
                                     FROM v3_3_configuration_history_clusters
                                       WHERE v3_3_configuration_history_clusters.datacenter_id = cast($P{P_DataCenter_ID} as uuid))
        AND v3_3_configuration_history_hosts.cluster_id = CASE $P{P_Cluster_ID}
                                 WHEN '11111111-1111-1111-1111-111111111111' then v3_3_configuration_history_hosts.cluster_id
                                 ELSE cast($P{P_Cluster_ID} as uuid)
                                     END
        AND history_datetime >= cast($P{P_Start_Date} as timestamp)
        AND history_datetime <= CASE
                                   WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                   WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                   WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                   WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                               END
            AND a.history_id in (SELECT max(b.history_id)
                         FROM v3_3_configuration_history_hosts b
                         GROUP BY b.host_id)
            AND CASE
                WHEN $P{is_deleted} like 'AND%' THEN a.delete_date IS NULL
                ELSE a.delete_date IS NULL or a.delete_date IS NOT NULL
                 END
        GROUP BY history_datetime, coalesce(v3_3_configuration_history_hosts.host_os, 'Unknown')) a right outer join calendar b on (a.history_datetime = b.$P!{calendar_column})
where $P!{calendar_column} >= cast($P{P_Start_Date} as date)
      AND $P!{calendar_column} <= CASE
                         WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                         WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                         WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                         WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                     END
group by $P!{calendar_column}, os_type, host_count
order by $P!{calendar_column}]]>
    </queryString>
