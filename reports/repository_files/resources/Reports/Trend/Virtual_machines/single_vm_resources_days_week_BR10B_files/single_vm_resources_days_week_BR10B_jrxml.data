    <subDataset name="Memory" uuid="b6b18129-5327-4b9a-a225-829b19fac31a">
        <parameter name="is_deleted" class="java.lang.String">
            <defaultValueExpression><![CDATA["AND delete_date IS NULL"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Period" class="java.lang.String">
            <parameterDescription><![CDATA[Period]]></parameterDescription>
            <defaultValueExpression><![CDATA["Daily"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Start_Date" class="java.util.Date">
            <defaultValueExpression><![CDATA["01/01/2000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_DataCenter_ID" class="java.lang.String">
            <parameterDescription><![CDATA[Select a DataCenter]]></parameterDescription>
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Cluster_ID" class="java.lang.String">
            <parameterDescription><![CDATA[Select a Cluster]]></parameterDescription>
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_VM_Type" class="java.lang.Integer">
            <defaultValueExpression><![CDATA[-1]]></defaultValueExpression>
        </parameter>
        <parameter name="P_VM_ID" class="java.lang.String">
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="table_name" class="java.lang.String">
            <defaultValueExpression><![CDATA[$P{P_Period} == "Daily" ? "hourly" : "daily"]]></defaultValueExpression>
        </parameter>
        <parameter name="calendar_column" class="java.lang.String">
            <defaultValueExpression><![CDATA[$P{P_Period} == "Daily" ? "the_datetime" : "the_date"]]></defaultValueExpression>
        </parameter>
        <queryString>
            <![CDATA[select distinct extract(DOW from $P!{calendar_column}) as day_num, day_name, avg(MEM_Usage) as MEM_Usage, avg(MEM_Peak) as MEM_Peak
from    (-- VMs Max and Average MEMory Usage Peaks
         SELECT history_datetime AS time,
                SUM(coalesce(memory_usage_percent,0)*coalesce(minutes_in_status,0))/SUM(coalesce(minutes_in_status,0)) AS MEM_Usage,
                SUM(coalesce(max_memory_usage,0)*coalesce(minutes_in_status,0))/SUM(coalesce(minutes_in_status,0)) AS MEM_Peak
         FROM v3_2_statistics_vms_resources_usage_$P!{table_name}
         WHERE v3_2_statistics_vms_resources_usage_$P!{table_name}.vm_id = CASE $P{P_VM_ID}
                                                                               WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                                                                               WHEN null THEN '00000000-0000-0000-0000-000000000000'
                                                                               WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                                                                               ELSE cast($P{P_VM_ID} as uuid)
                                                                           END
               AND history_datetime >= cast($P{P_Start_Date} as date)
               AND history_datetime <= CASE
                                           WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                           WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                           WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                           WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                       END
         GROUP BY history_datetime) a RIGHT OUTER JOIN calendar ON ($P!{calendar_column} = time)
WHERE $P!{calendar_column} >= cast($P{P_Start_Date} as date)
      AND $P!{calendar_column} <= CASE
                                      WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                      WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                      WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                      WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                  END
GROUP BY extract(DOW from $P!{calendar_column}), day_name
ORDER BY extract(DOW from $P!{calendar_column}) ASC]]>
        </queryString>
        <field name="day_num" class="java.lang.Double"/>
        <field name="day_name" class="java.lang.String"/>
        <field name="mem_usage" class="java.math.BigDecimal"/>
        <field name="mem_peak" class="java.math.BigDecimal"/>
    </subDataset>
    <subDataset name="Disk_usage" uuid="4f2a66e3-b624-432f-9037-83a8bbbbaa64">
        <parameter name="is_deleted" class="java.lang.String">
            <defaultValueExpression><![CDATA["AND delete_date IS NULL"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Period" class="java.lang.String">
            <parameterDescription><![CDATA[Period]]></parameterDescription>
            <defaultValueExpression><![CDATA["Daily"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Start_Date" class="java.util.Date">
            <defaultValueExpression><![CDATA["01/01/2000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_DataCenter_ID" class="java.lang.String">
            <parameterDescription><![CDATA[Select a DataCenter]]></parameterDescription>
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Cluster_ID" class="java.lang.String">
            <parameterDescription><![CDATA[Select a Cluster]]></parameterDescription>
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_VM_Type" class="java.lang.Integer">
            <defaultValueExpression><![CDATA[-1]]></defaultValueExpression>
        </parameter>
        <parameter name="P_VM_ID" class="java.lang.String">
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="table_name" class="java.lang.String">
            <defaultValueExpression><![CDATA[$P{P_Period} == "Daily" ? "hourly" : "daily"]]></defaultValueExpression>
        </parameter>
        <parameter name="calendar_column" class="java.lang.String">
            <defaultValueExpression><![CDATA[$P{P_Period} == "Daily" ? "the_datetime" : "the_date"]]></defaultValueExpression>
        </parameter>
        <queryString>
            <![CDATA[select distinct extract(DOW from $P!{calendar_column}) as day_num, day_name, coalesce(disk_name, max(disk_name) over (partition by 1), '') as disk_name, avg(Read_Rate) as Read_Rate, avg(Write_Rate) as Write_Rate
from   (-- VMs Disk Usage (I/O) Rates
        SELECT CASE
                   WHEN delete_date IS NULL THEN vm_disk_name
                    ELSE vm_disk_name || '*'
                END as disk_name,
                extract(DOW from history_datetime) AS Time,
                MAX(ROUND(CAST(coalesce(read_rate_bytes_per_second, 0) AS FLOAT)::numeric /(1024*1024),1)) AS Read_Rate,
                MAX(ROUND(CAST(coalesce(write_rate_bytes_per_second, 0) AS FLOAT)::numeric /(1024*1024),1)) AS Write_Rate
        FROM v3_2_statistics_vms_disks_resources_usage_$P!{table_name}
             INNER JOIN v3_2_configuration_history_vms_disks
                 ON (v3_2_configuration_history_vms_disks.vm_disk_id = v3_2_statistics_vms_disks_resources_usage_$P!{table_name}.vm_disk_id)
        WHERE history_datetime >= cast($P{P_Start_Date} as timestamp)
              AND history_datetime <= CASE
                                          WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                          WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                          WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                          WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                      END
              AND v3_2_configuration_history_vms_disks.history_id in (SELECT max(a.history_id)
                                                                      FROM v3_2_configuration_history_vms_disks as a
                                                                      GROUP BY a.vm_disk_id)
              AND v3_2_configuration_history_vms_disks.vm_disk_id in (SELECT c.vm_disk_id
                                                                      FROM v3_2_statistics_vms_disks_resources_usage_$P!{table_name} b
                                                                           INNER JOIN v3_2_configuration_history_vms_disks c ON (c.vm_disk_id = b.vm_disk_id)
                                                                      WHERE c.vm_disk_id in (SELECT device_id
                                                                                            FROM v3_2_configuration_history_vms_devices
                                                                                            WHERE vm_id = CASE $P{P_VM_ID}
                                                                                                              WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                                                                                                              WHEN null THEN '00000000-0000-0000-0000-000000000000'
                                                                                                              WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                                                                                                              ELSE cast($P{P_VM_ID} as uuid)
                                                                                                          END
                                                                                                  AND create_date <= CASE
                                                                                                                         WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                                                                         WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                                                                         WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                                                                         WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                                                                     END
                                                                                                  AND (delete_date <= CAST($P{P_Start_Date} as TIMESTAMP) OR delete_date IS NULL)
                                                                                                  AND history_id in (SELECT max(a.history_id)
                                                                                                                     FROM v3_2_configuration_history_vms_devices a
                                                                                                                     GROUP BY a.vm_id, a.device_id)
                                                                                                  $P!{is_deleted})
                                                                            AND history_datetime >= cast($P{P_Start_Date} as timestamp)
                                                                            AND history_datetime <= CASE
                                                                                                        WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                                                        WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                                                        WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                                                        WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                                                    END
                                                                            AND c.history_id in (SELECT max(d.history_id)
                                                                                                 FROM  v3_2_configuration_history_vms_disks as d
                                                                                                 GROUP BY d.vm_disk_id)
                                                                            $P!{is_deleted}
                                                                      GROUP BY c.vm_disk_id
                                                                      ORDER BY MAX(ROUND(CAST(coalesce(b.read_rate_bytes_per_second, 0) AS FLOAT)::numeric,1)) + MAX(ROUND(CAST(coalesce(b.write_rate_bytes_per_second, 0) AS FLOAT)::numeric,1)) DESC
                                                                      LIMIT 5)
              $P!{is_deleted}
        GROUP BY v3_2_statistics_vms_disks_resources_usage_$P!{table_name}.vm_disk_id, disk_name, extract(DOW from history_datetime)) a RIGHT OUTER JOIN calendar ON (extract(DOW from $P!{calendar_column}) = time)
WHERE $P!{calendar_column} >= cast($P{P_Start_Date} as date)
      AND $P!{calendar_column} <= CASE
                                      WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                      WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                      WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                      WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                  END
GROUP BY extract(DOW FROM $P!{calendar_column}), day_name, disk_name
ORDER BY extract(DOW FROM $P!{calendar_column}), coalesce(disk_name, max(disk_name) over (partition by 1), '')]]>
        </queryString>
        <field name="day_num" class="java.lang.Double"/>
        <field name="day_name" class="java.lang.String"/>
        <field name="disk_name" class="java.lang.String"/>
        <field name="read_rate" class="java.math.BigDecimal"/>
        <field name="write_rate" class="java.math.BigDecimal"/>
    </subDataset>
    <subDataset name="Interfaces" uuid="22704933-ca4e-4d20-a826-559fde221371">
        <parameter name="is_deleted" class="java.lang.String">
            <defaultValueExpression><![CDATA["AND delete_date IS NULL"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Period" class="java.lang.String">
