    <queryString>
        <![CDATA[-- VMs Max and Average CPU Usage Peaks

select $P!{calendar_column} as the_date, CPU_Usage, CPU_Peak

from    (SELECT
        history_datetime AS time,
              SUM(coalesce(cpu_usage_percent,0)*coalesce(minutes_in_status,0))/SUM(coalesce(minutes_in_status,0)) AS CPU_Usage,
        SUM(coalesce(max_cpu_usage,0)*coalesce(minutes_in_status,0))/SUM(coalesce(minutes_in_status,0)) AS CPU_Peak
    FROM v3_0_vm_$P!{table_name}_history_view
    WHERE
        v3_0_vm_$P!{table_name}_history_view.vm_id =
            CASE $P{P_VM_ID}
                WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                WHEN null THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                ELSE cast($P{P_VM_ID} as uuid)
            END
        AND history_datetime >= cast($P{P_Start_Date} as timestamp)
        AND history_datetime <= CASE
                                   WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                   WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                   WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                   WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                               END
    GROUP By history_datetime) a RIGHT OUTER JOIN calendar ON ($P!{calendar_column} = time)

