	<queryString>
		<![CDATA[SELECT 	vm_name,
	cast(delete_date as date) as vm_delete_date,
	cast(create_date as date) as vm_create_date,
	high_availability,
	round(cast(memory_size_mb as float)::numeric/1024,1) as memory_size,
	(cpu_per_socket*number_of_sockets)  as number_of_cpus,
	(SELECT v3_0_enum_translator_view.value
		           FROM v3_0_enum_translator_view
		           WHERE v3_0_enum_translator_view.enum_key = v3_0_vm_configuration_view.operating_system
			         AND v3_0_enum_translator_view.enum_type = 'OS_TYPE')  as operating_system,
        coalesce((SELECT  ' ' || v3_0_cluster_configuration_view.cluster_name
		  FROM v3_0_cluster_configuration_view
	          WHERE v3_0_cluster_configuration_view.cluster_id = cast($P{P_Cluster_ID} as uuid)
		        and history_id in (select max(b.history_id)
			    		   from v3_0_cluster_configuration_view as b
			   		   where b.cluster_id = v3_0_cluster_configuration_view.cluster_id)), 'all') AS cluster_name,
        (SELECT cast(v3_0_cluster_configuration_view.delete_date as date)
         FROM v3_0_cluster_configuration_view
         WHERE 	v3_0_cluster_configuration_view.cluster_id = cast($P{P_Cluster_ID} as uuid)
		and history_id in (select max(c.history_id)
		 		   from v3_0_cluster_configuration_view as c
		   		   where c.cluster_id = v3_0_cluster_configuration_view.cluster_id)) as cluster_delete_date,
        coalesce((SELECT v3_0_enum_translator_view.value
	          FROM v3_0_enum_translator_view
		  WHERE v3_0_enum_translator_view.enum_key = v3_0_vm_configuration_view.vm_type
		  AND v3_0_enum_translator_view.enum_type = 'VM_TYPE'), 'All') as vm_type,
       (SELECT v3_0_datacenter_configuration_view.datacenter_name
        FROM v3_0_datacenter_configuration_view
	WHERE v3_0_datacenter_configuration_view.datacenter_id = cast($P{P_DataCenter_ID} as uuid)
	      and history_id in (select max(d.history_id)
			    	 from v3_0_datacenter_configuration_view as d
			   	 where d.datacenter_id = v3_0_datacenter_configuration_view.datacenter_id)) as datacenter_name,
       (SELECT CAST(v3_0_datacenter_configuration_view.delete_date as date)
	FROM v3_0_datacenter_configuration_view
	WHERE v3_0_datacenter_configuration_view.datacenter_id = cast($P{P_DataCenter_ID} as uuid)
	      and history_id in (select max(d.history_id)
			    	 from v3_0_datacenter_configuration_view as d
			   	 where d.datacenter_id = v3_0_datacenter_configuration_view.datacenter_id)) as datacenter_delete_date
FROM v3_0_vm_configuration_view
WHERE 	vm_id = cast($P{P_VM_ID} as uuid)
	and history_id in (select max(a.history_id)
			   from v3_0_vm_configuration_view as a
			   where a.vm_id = v3_0_vm_configuration_view.vm_id)]]>
	</queryString>
