    <queryString>
        <![CDATA[SELECT    v3_2_configuration_history_datacenters.datacenter_name as datacenter_name,
         v3_2_configuration_history_datacenters.delete_date as datacenter_delete_date,
          coalesce((SELECT ' ' || RTrim(cast(v3_2_configuration_history_clusters.cluster_name as varchar))
          FROM v3_2_configuration_history_clusters
              WHERE v3_2_configuration_history_clusters.cluster_id = cast($P{P_Cluster_ID} as uuid)
                and v3_2_configuration_history_clusters.history_id in (SELECT max(a.history_id)
                                       FROM v3_2_configuration_history_clusters as a
                                       GROUP BY a.cluster_id)), 'all') as cluster_name,
        (SELECT v3_2_configuration_history_clusters.delete_date
         FROM v3_2_configuration_history_clusters
         WHERE v3_2_configuration_history_clusters.cluster_id = cast($P{P_Cluster_ID} as uuid)
          and v3_2_configuration_history_clusters.history_id in (SELECT max(a.history_id)
                                    FROM v3_2_configuration_history_clusters as a
                                    GROUP BY a.cluster_id)) as cluster_delete_date,
        coalesce((SELECT v3_2_enum_translator.value
              FROM v3_2_enum_translator
          WHERE v3_2_enum_translator.enum_key = $P{P_VM_Type}
          AND v3_2_enum_translator.enum_type = 'VM_TYPE'), 'All') as vm_type,
        CASE
            WHEN $P{P_Period} = 'Monthly' THEN CAST(cast($P{P_Start_Date} as date) + interval '1 month' - interval '1 day' as date)
            WHEN $P{P_Period} = 'Quarterly' THEN CAST(cast($P{P_Start_Date} as date) + interval '3 month' - interval '1 day' as date)
            WHEN $P{P_Period} = 'Yearly' THEN CAST(cast($P{P_Start_Date} as date) + interval '1 year' - interval '1 day' as date)
        END as end_date
FROM v3_2_configuration_history_datacenters
WHERE     v3_2_configuration_history_datacenters.datacenter_id = cast($P{P_DataCenter_ID} as uuid)
    and v3_2_configuration_history_datacenters.history_id in (SELECT max(a.history_id)
                                 FROM v3_2_configuration_history_datacenters as a
                                 GROUP BY a.datacenter_id)]]>
    </queryString>
