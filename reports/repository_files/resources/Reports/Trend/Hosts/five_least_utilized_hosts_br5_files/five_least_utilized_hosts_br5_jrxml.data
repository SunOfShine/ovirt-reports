    <queryString>
        <![CDATA[-- CPU Usage weighted peak of the top 5 busiest hosts
select distinct $P!{calendar_column} as the_date, coalesce(host_name,max(host_name) over (partition by 1),'') as host_name, CPU_Usage, MEM_Usage
from (SELECT v3_0_host_$P!{table_name}_history_view.host_id,
            CASE
                WHEN delete_date IS NULL THEN host_name
                ELSE host_name || ' (Removed on ' || cast(cast(delete_date as date) as varchar) || ')'
            END as host_name,
            v3_0_host_$P!{table_name}_history_view.history_datetime AS time,
            SUM(coalesce(v3_0_host_$P!{table_name}_history_view.max_cpu_usage, 0)*coalesce(v3_0_host_$P!{table_name}_history_view.minutes_in_status,0))/SUM(coalesce(v3_0_host_$P!{table_name}_history_view.minutes_in_status,0)) AS CPU_Usage,
            SUM(coalesce(v3_0_host_$P!{table_name}_history_view.max_memory_usage,0)*coalesce(v3_0_host_$P!{table_name}_history_view.minutes_in_status,0))/SUM(coalesce(v3_0_host_$P!{table_name}_history_view.minutes_in_status,0)) AS MEM_Usage
    FROM v3_0_host_$P!{table_name}_history_view
            INNER JOIN v3_0_host_configuration_view
                ON (v3_0_host_configuration_view.host_id = v3_0_host_$P!{table_name}_history_view.host_id)
    WHERE    v3_0_host_$P!{table_name}_history_view.history_datetime >= cast($P{P_Start_Date} as timestamp)
            AND v3_0_host_$P!{table_name}_history_view.history_datetime <= CASE
                                                                    WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                    WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                    WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                    WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                END
            AND v3_0_host_configuration_view.history_id in (SELECT max(a.history_id)
                                    FROM v3_0_host_configuration_view as a
                                    GROUP BY a.host_id)
            AND v3_0_host_$P!{table_name}_history_view.host_id IN
                ( -- Calculation of Top 5 Hosts
                 SELECT a.host_id
                 FROM v3_0_host_$P!{table_name}_history_view a
                        INNER JOIN v3_0_host_configuration_view b
                            ON (a.host_id = b.host_id)
                 WHERE    b.cluster_id in (SELECT v3_0_cluster_configuration_view.cluster_id
                                         FROM v3_0_cluster_configuration_view
                                         WHERE v3_0_cluster_configuration_view.datacenter_id = cast($P{P_DataCenter_ID} as uuid))
                        AND b.cluster_id = CASE $P{P_Cluster_ID}
                                             WHEN '11111111-1111-1111-1111-111111111111' then b.cluster_id
                                             ELSE cast($P{P_Cluster_ID} as uuid)
                                           END
                        AND b.host_type = CASE $P{P_Host_Type}
                                                WHEN -1 THEN b.host_type
                                                 WHEN NULL THEN b.host_type
                                          ELSE $P{P_Host_Type}
                                          END
                        AND a.history_datetime >= cast($P{P_Start_Date} as timestamp)
                        AND a.history_datetime <= CASE
                                                     WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                     WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                     WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                     WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                 END
                        AND b.history_id in (SELECT max(g.history_id)
                                     FROM v3_0_host_configuration_view g
                                     GROUP BY g.host_id)
                        $P!{is_deleted}
                GROUP BY a.host_id
                ORDER BY SUM(coalesce(a.max_memory_usage*a.minutes_in_status,0))/SUM(coalesce(a.minutes_in_status,0)) +
                          SUM(coalesce(a.max_cpu_usage*a.minutes_in_status,0))/SUM(coalesce(a.minutes_in_status,0)) ASC
                LIMIT 5)
    GROUP BY v3_0_host_$P!{table_name}_history_view.host_id, host_name, delete_date, history_datetime) a right outer join calendar b on (a.time = b.$P!{calendar_column})
where $P!{calendar_column} >= cast($P{P_Start_Date} as date)
      AND $P!{calendar_column} <= CASE
                         WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                         WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                         WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                         WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                      END
order by $P!{calendar_column}]]>
    </queryString>
