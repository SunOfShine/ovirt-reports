        <queryString>
            <![CDATA[select distinct cast(extract(HOUR from the_datetime) as int)as hour_num, avg(MEM_Usage) as MEM_Usage, avg(MEM_Peak) as MEM_Peak
from    (-- Hosts Max and Average MEMory Usage Peaks
    SELECT
        history_datetime AS time,
            SUM(coalesce(memory_usage_percent,0)*coalesce(minutes_in_status,0))/SUM(coalesce(minutes_in_status,0)) AS MEM_Usage,
            SUM(coalesce(max_memory_usage,0)*coalesce(minutes_in_status,0))/SUM(coalesce(minutes_in_status,0)) AS MEM_Peak
    FROM v3_3_statistics_hosts_resources_usage_hourly

    WHERE
        v3_3_statistics_hosts_resources_usage_hourly.host_id =
            CASE $P{P_Host_ID}
                WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                WHEN null THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                ELSE cast($P{P_Host_ID} as uuid)
            END
        AND history_datetime >= cast($P{P_Start_Date} as date)
        AND history_datetime <= CASE
                                   WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                   WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                   WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                   WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                               END
    GROUP BY
        history_datetime) a RIGHT OUTER JOIN calendar ON (the_datetime = time)
where     the_datetime >= cast($P{P_Start_Date} as date)
    AND the_datetime < CASE
                           WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                           WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                           WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                           WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                       END
group by cast(extract(HOUR from the_datetime) as int)
order by cast(extract(HOUR from the_datetime) as int) ASC]]>
        </queryString>
        <field name="hour_num" class="java.lang.Integer"/>
        <field name="mem_usage" class="java.math.BigDecimal"/>
        <field name="mem_peak" class="java.math.BigDecimal"/>
    </subDataset>
