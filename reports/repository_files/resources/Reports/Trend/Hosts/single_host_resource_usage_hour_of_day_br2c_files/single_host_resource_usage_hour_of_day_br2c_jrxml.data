        </parameter>
        <queryString>
            <![CDATA[select distinct cast(extract(HOUR from the_datetime) as int) as hour_num, coalesce(interface_name, max(interface_name) over (partition by 1),'') as interface_name, avg(Rx_rate) as Rx_rate, avg(Tx_rate) as Tx_rate
from    (-- Hosts Ethernet Interface Tx and Rx Rates
    SELECT
        v3_3_configuration_history_hosts_interfaces.host_interface_id,
        CASE
            WHEN delete_date IS NULL THEN v3_3_configuration_history_hosts_interfaces.host_interface_name
            ELSE v3_3_configuration_history_hosts_interfaces.host_interface_name || '*'
        END as interface_name,
        v3_3_statistics_hosts_interfaces_resources_usage_hourly.history_datetime AS time,
        avg(coalesce(receive_rate_percent,0)) AS Rx_rate,
        avg(coalesce(transmit_rate_percent,0)) AS Tx_rate
    FROM v3_3_statistics_hosts_interfaces_resources_usage_hourly
        INNER JOIN v3_3_configuration_history_hosts_interfaces
            ON (v3_3_configuration_history_hosts_interfaces.host_interface_id = v3_3_statistics_hosts_interfaces_resources_usage_hourly.host_interface_id)
    WHERE
        v3_3_configuration_history_hosts_interfaces.host_id =
            CASE $P{P_Host_ID}
                WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                WHEN null THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                ELSE cast($P{P_Host_ID} as uuid)
            END
        AND history_datetime >= cast($P{P_Start_Date} as timestamp)
        AND history_datetime <= CASE
                                   WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                   WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                   WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                   WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                               END
        AND v3_3_configuration_history_hosts_interfaces.history_id in (SELECT max(a.history_id)
                                   FROM v3_3_configuration_history_hosts_interfaces as a
                                   GROUP BY a.host_interface_id)
        AND v3_3_configuration_history_hosts_interfaces.host_interface_id in (SELECT c.host_interface_id
                                     FROM v3_3_statistics_hosts_interfaces_resources_usage_hourly b
                                    INNER JOIN v3_3_configuration_history_hosts_interfaces c
                                        ON (c.host_interface_id = b.host_interface_id)
                                   WHERE     c.host_id = CASE $P{P_Host_ID}
                                            WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                                            WHEN null THEN '00000000-0000-0000-0000-000000000000'
                                            WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                                            ELSE cast($P{P_Host_ID} as uuid)
                                          END
                                    AND history_datetime >= cast($P{P_Start_Date} as timestamp)
                                    AND history_datetime <= CASE
                                                               WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                               WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                               WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                               WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                           END
                                    AND c.history_id in (SELECT max(d.history_id)
                                             FROM v3_3_configuration_history_hosts_interfaces as d
                                             GROUP BY d.host_interface_id)
                                    $P!{is_deleted}
                                  Group BY c.host_interface_id
                                  ORDER BY avg(coalesce(b.receive_rate_percent,0)) + avg(coalesce(b.transmit_rate_percent,0)) DESC
                                  LIMIT 3)
        $P!{is_deleted}
    Group BY
        v3_3_configuration_history_hosts_interfaces.host_interface_id, interface_name,
        v3_3_statistics_hosts_interfaces_resources_usage_hourly.history_datetime) as a RIGHT OUTER JOIN calendar as b ON (a.time = b.the_datetime)
where b.the_datetime >= cast($P{P_Start_Date} as timestamp)
      AND b.the_datetime < CASE
                               WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                               WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                               WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                               WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                           END
group by cast(extract(HOUR from the_datetime) as int), interface_name
order by cast(extract(HOUR from the_datetime) as int) ASC]]>
        </queryString>
        <field name="hour_num" class="java.lang.Integer"/>
        <field name="interface_name" class="java.lang.String"/>
        <field name="rx_rate" class="java.math.BigDecimal"/>
        <field name="tx_rate" class="java.math.BigDecimal"/>
    </subDataset>
