    <queryString>
            <![CDATA[select $P!{calendar_column} as the_date, coalesce(interface_name, max(interface_name) over (partition by 1),'') as interface_name, Rx_rate, Tx_rate
from    (-- Hosts Ethernet Interface Tx and Rx Rates
    SELECT
        v3_0_host_interface_configuration_view.host_interface_id,
        CASE
            WHEN delete_date IS NULL THEN v3_0_host_interface_configuration_view.host_interface_name
            ELSE v3_0_host_interface_configuration_view.host_interface_name || '*'
        END as interface_name,
        v3_0_host_interface_$P!{table_name}_history_view.history_datetime AS time,
        avg(coalesce(receive_rate_percent,0)) AS Rx_rate,
        avg(coalesce(transmit_rate_percent,0)) AS Tx_rate
    FROM v3_0_host_interface_$P!{table_name}_history_view
        INNER JOIN v3_0_host_interface_configuration_view
            ON (v3_0_host_interface_configuration_view.host_interface_id = v3_0_host_interface_$P!{table_name}_history_view.host_interface_id)
    WHERE
        v3_0_host_interface_configuration_view.host_id =
            CASE $P{P_Host_ID}
                WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                WHEN null THEN '00000000-0000-0000-0000-000000000000'
                WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                ELSE cast($P{P_Host_ID} as uuid)
            END
        AND history_datetime >= cast($P{P_Start_Date} as timestamp)
        AND history_datetime <= CASE
                                   WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                   WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                   WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                   WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                               END
        AND v3_0_host_interface_configuration_view.history_id in (SELECT max(a.history_id)
                                   FROM v3_0_host_interface_configuration_view as a
                                   GROUP BY a.host_interface_id)
        AND v3_0_host_interface_configuration_view.host_interface_id in (SELECT c.host_interface_id
                                     FROM v3_0_host_interface_$P!{table_name}_history_view b
                                    INNER JOIN v3_0_host_interface_configuration_view c
                                        ON (c.host_interface_id = b.host_interface_id)
                                   WHERE     c.host_id = CASE $P{P_Host_ID}
                                            WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                                            WHEN null THEN '00000000-0000-0000-0000-000000000000'
                                            WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                                            ELSE cast($P{P_Host_ID} as uuid)
                                          END
                                    AND history_datetime >= cast($P{P_Start_Date} as timestamp)
                                    AND history_datetime <= CASE
                                                               WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                               WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                               WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                               WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                           END
                                    AND c.history_id in (SELECT max(d.history_id)
                                             FROM v3_0_host_interface_configuration_view as d
                                             GROUP BY d.host_interface_id)
                                    $P!{is_deleted}
                                  Group BY c.host_interface_id
                                  ORDER BY avg(coalesce(b.receive_rate_percent,0)) + avg(coalesce(b.transmit_rate_percent,0)) DESC
                                  LIMIT 5)
        $P!{is_deleted}
    Group BY
        v3_0_host_interface_configuration_view.host_interface_id, interface_name,
        v3_0_host_interface_$P!{table_name}_history_view.history_datetime) as a RIGHT OUTER JOIN calendar as b ON (a.time = b.$P!{calendar_column})
where b.$P!{calendar_column} >= cast($P{P_Start_Date} as timestamp)
      AND b.$P!{calendar_column} <= CASE
                           WHEN $P{P_Period} = 'Daily' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                           WHEN $P{P_Period} = 'Monthly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                           WHEN $P{P_Period} = 'Quarterly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                           WHEN $P{P_Period} = 'Yearly' THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                       END
GROUP BY  $P!{calendar_column}, interface_name, Rx_rate, Tx_rate
ORDER BY $P!{calendar_column}]]>
        </queryString>
        <field name="the_date" class="java.sql.Timestamp"/>
        <field name="interface_name" class="java.lang.String"/>
        <field name="rx_rate" class="java.math.BigDecimal"/>
        <field name="tx_rate" class="java.math.BigDecimal"/>
    </subDataset>
