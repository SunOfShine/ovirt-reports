    <subDataset name="Table" uuid="88123fea-ab67-4b33-9161-f8a32069d747">
        <parameter name="P_DataCenter_ID" class="java.lang.String">
            <parameterDescription><![CDATA[Select a DataCenter]]></parameterDescription>
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Cluster_ID" class="java.lang.String">
            <parameterDescription><![CDATA[Select a Cluster]]></parameterDescription>
            <defaultValueExpression><![CDATA["00000000-0000-0000-0000-000000000000"]]></defaultValueExpression>
        </parameter>
        <parameter name="P_Host_Type" class="java.lang.Integer">
            <defaultValueExpression><![CDATA[-1]]></defaultValueExpression>
        </parameter>
        <parameter name="active_hosts_select" class="java.lang.String">
            <defaultValueExpression><![CDATA["AND delete_date IS NULL"]]></defaultValueExpression>
        </parameter>
        <queryString>
            <![CDATA[SELECT
      v3_2_configuration_history_clusters.cluster_name
      ,v3_2_configuration_history_hosts.host_name
      ,v3_2_configuration_history_hosts.fqdn_or_ip
      ,cast(v3_2_configuration_history_hosts.create_date as date) as create_date
      ,cast(v3_2_configuration_history_hosts.delete_date as date) as delete_date
      ,v3_2_configuration_history_hosts.number_of_cores
      ,v3_2_configuration_history_hosts.vdsm_version
      ,v3_2_configuration_history_hosts.host_os
      ,v3_2_configuration_history_hosts.cluster_id
      ,v3_2_configuration_history_hosts.cpu_model
      ,v3_2_enum_translator.value as host_type
      ,v3_2_configuration_history_clusters.delete_date as cluster_delete
      ,round(cast(coalesce(memory_size_mb, 0.00) as float)::numeric/1024,1) AS memory_size_mb
FROM v3_2_configuration_history_hosts
         LEFT OUTER JOIN v3_2_enum_translator ON (v3_2_configuration_history_hosts.host_type = v3_2_enum_translator.enum_key AND v3_2_enum_translator.enum_type = 'HOST_TYPE')
         LEFT OUTER JOIN v3_2_configuration_history_clusters ON (v3_2_configuration_history_hosts.cluster_id = v3_2_configuration_history_clusters.cluster_id
                                                             AND v3_2_configuration_history_clusters.history_id in (SELECT max(a.history_id)
                                                                                                                FROM v3_2_configuration_history_clusters a
                                                                                                                GROUP BY a.cluster_id))
WHERE v3_2_configuration_history_hosts.cluster_id in (SELECT a.cluster_id
                                                  FROM v3_2_configuration_history_clusters a
                                                  WHERE a.datacenter_id = cast($P{P_DataCenter_ID} as uuid))
      AND v3_2_configuration_history_hosts.cluster_id = CASE cast($P{P_Cluster_ID} as uuid)
                                                        WHEN NULL THEN v3_2_configuration_history_hosts.cluster_id
                                                        WHEN CAST('11111111-1111-1111-1111-111111111111' as uuid) THEN v3_2_configuration_history_hosts.cluster_id
                                                        ELSE cast($P{P_Cluster_ID} as uuid)
                                                    END
      AND v3_2_configuration_history_hosts.host_type = CASE $P{P_Host_Type}
                                                       WHEN -1 THEN v3_2_configuration_history_hosts.host_type
                                                       ELSE $P{P_Host_Type}
                                                   END
      AND v3_2_configuration_history_hosts.history_id in (SELECT max(a.history_id)
                                                      FROM v3_2_configuration_history_hosts a
                                                      GROUP BY a.host_id)
      AND CASE
              WHEN $P{active_hosts_select} like '%delete%' THEN v3_2_configuration_history_hosts.delete_date IS NULL
              ELSE v3_2_configuration_history_hosts.delete_date IS NULL or v3_2_configuration_history_hosts.delete_date IS NOT NULL
          END
ORDER BY v3_2_configuration_history_hosts.cluster_id, v3_2_configuration_history_hosts.delete_date DESC, v3_2_configuration_history_hosts.host_name]]>
        </queryString>
