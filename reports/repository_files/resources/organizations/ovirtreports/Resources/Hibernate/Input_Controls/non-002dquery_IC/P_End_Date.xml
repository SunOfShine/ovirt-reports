<?xml version="1.0" encoding="UTF-8"?>
<inputControl>
    <folder>/organizations/ovirtreports/Resources/Hibernate/Input_Controls/non-query_IC</folder>
    <name>P_End_Date</name>
    <version>3</version>
    <label>End Date</label>
    <description></description>
    <creationDate>2011-10-18T16:58:20.000+02:00</creationDate>
    <updateDate>2011-10-18T17:50:18.000+02:00</updateDate>
    <type>4</type>
    <mandatory>true</mandatory>
    <readOnly>true</readOnly>
    <visible>false</visible>
    <query>
        <localResource
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="query">
            <folder>/organizations/ovirtreports/Resources/Hibernate/Input_Controls/non-query_IC/P_End_Date_files</folder>
            <name>monthEnd</name>
            <version>1</version>
            <label>monthEnd</label>
            <description></description>
            <creationDate>2011-10-18T17:50:18.000+02:00</creationDate>
            <updateDate>2011-10-18T17:50:18.000+02:00</updateDate>
            <language>hql</language>
            <queryString>select CASE
	WHEN $P{P_Period} = 'Monthly' THEN o.monthEnddate 
	WHEN $P{P_Period} = 'Quarterly' THEN o.quarterEnddate 
           END as Enddate
from Period as o
where o.monthName = $P{P_Dates}</queryString>
            <dataSource>
                <uri>/organizations/ovirtreports/Resources/Hibernate/data_sources/ovirt-hibernate</uri>
            </dataSource>
        </localResource>
    </query>
    <queryVisibleColumn>Enddate</queryVisibleColumn>
    <queryValueColumn>Enddate</queryValueColumn>
</inputControl>
