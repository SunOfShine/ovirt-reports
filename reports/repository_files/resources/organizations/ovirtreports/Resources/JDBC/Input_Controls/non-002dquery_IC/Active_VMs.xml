            <updateDate>2011-12-04T19:43:38.460+02:00</updateDate>
            <language>sql</language>
            <queryString>select value as display, 'AND delete_date IS NULL' as value
from v3_0_enum_translator_view
where enum_type = 'REPORTS_SHOW_DELETED'
          and enum_key = 0
UNION ALL
select value, '/**/'
from v3_0_enum_translator_view
where enum_type = 'REPORTS_SHOW_DELETED'
          and enum_key = 1</queryString>
