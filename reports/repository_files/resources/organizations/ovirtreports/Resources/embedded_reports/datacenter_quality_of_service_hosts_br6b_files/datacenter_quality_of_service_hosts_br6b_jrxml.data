	<queryString>
		<![CDATA[SELECT *
FROM   (SELECT 	cluster_id,
		cluster_name,
		is_delete,
		cluster_delete_only_date,
		avg(time_above_cpu_threshold) as time_above_cpu_threshold,
		avg(time_above_mem_threshold) as time_above_mem_threshold,
		avg(time_below_cpu_threshold) as time_below_cpu_threshold,
		avg(time_below_mem_threshold) as time_below_mem_threshold,
		avg(overall_host_time) as overall_cluster_time
	FROM	(SELECT v3_0_host_configuration_view.cluster_id,
			v3_0_cluster_configuration_view.cluster_name,
			(CASE WHEN v3_0_cluster_configuration_view.delete_date IS NULL THEN 0 ELSE 1 END) as is_delete,
			v3_0_host_hourly_history_view.host_id,
			CAST(v3_0_cluster_configuration_view.delete_date as date) as cluster_delete_only_date,
			SUM(CASE
				WHEN cpu_usage_percent >= $P{P_CPU_Threshold} THEN minutes_in_status
				ELSE 0
			     END) as time_above_cpu_threshold,
			SUM (CASE
				WHEN memory_usage_percent >= $P{P_MEM_Threshold} THEN minutes_in_status
				ELSE 0
			     END) as time_above_mem_threshold,
			SUM(CASE
				WHEN cpu_usage_percent < $P{P_CPU_Threshold} THEN minutes_in_status
				ELSE 0
			     END) as time_below_cpu_threshold,
			SUM (CASE
				WHEN memory_usage_percent < $P{P_MEM_Threshold} THEN minutes_in_status
				ELSE 0
			     END) as time_below_mem_threshold,
			SUM(coalesce(minutes_in_status, 0)) as overall_host_time
		FROM v3_0_host_hourly_history_view
			INNER JOIN v3_0_host_configuration_view
				ON (v3_0_host_hourly_history_view.host_id = v3_0_host_configuration_view.host_id)
					INNER JOIN v3_0_cluster_configuration_view
						ON (v3_0_cluster_configuration_view.cluster_id = v3_0_host_configuration_view.cluster_id)
		WHERE	v3_0_host_hourly_history_view.host_status = 1
			AND v3_0_host_configuration_view.cluster_id in (SELECT cluster_id
								        FROM v3_0_cluster_configuration_view
									WHERE datacenter_id = cast($P{P_DataCenter_ID} as uuid))
			AND v3_0_host_hourly_history_view.history_datetime >= cast($P{P_Start_Date} as timestamp)
			AND v3_0_host_hourly_history_view.history_datetime < cast($P{P_End_Date} as date) + interval '1 day'
			AND v3_0_cluster_configuration_view.history_id = (SELECT max(a.history_id)
									  FROM v3_0_cluster_configuration_view a
									  WHERE a.cluster_id = v3_0_cluster_configuration_view.cluster_id)
			AND v3_0_host_configuration_view.history_id =  (SELECT max(b.history_id)
									FROM v3_0_host_configuration_view b
									WHERE b.host_id = v3_0_host_configuration_view.host_id)
			AND CASE
				WHEN $P{active_hosts_select} like 'AND%' THEN v3_0_host_configuration_view.delete_date IS NULL
				ELSE v3_0_host_configuration_view.delete_date IS NULL or v3_0_host_configuration_view.delete_date IS NOT NULL
			    END
		GROUP BY	v3_0_host_configuration_view.cluster_id,
				v3_0_cluster_configuration_view.cluster_name,
				v3_0_cluster_configuration_view.delete_date,
				v3_0_host_hourly_history_view.host_id) a
	GROUP BY cluster_id,
		 cluster_name,
		 is_delete,
		 cluster_delete_only_date) t
WHERE t.overall_cluster_time > 0
ORDER BY (CASE WHEN time_above_mem_threshold = 0 and time_above_cpu_threshold = 0 THEN 1 ELSE 0 END) ASC, t.is_delete ASC, (cast(t.time_above_cpu_threshold as float)/cast(t.overall_cluster_time as float))*100 + (cast(t.time_above_mem_threshold as float)/cast(t.overall_cluster_time as float))*100 DESC]]>
	</queryString>
